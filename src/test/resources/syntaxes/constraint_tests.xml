<?xml version="1.0" encoding="UTF-8" ?>
<syntax>
  <tableCreation>
    <options>
      <inlineConstraintDefinitions>ALWAYS</inlineConstraintDefinitions>
    </options>
    <templates>
      <createTable>
        CREATE TABLE ${table_name} (${column_definitions} ${end_of_block_constraints});
      </createTable>
      <columnDefinition>
        ${column_name} ${column_type} ${end_of_line_constraints}
      </columnDefinition>
      <inlineConstraints>
        <constraint constraintType="NOT_NULL">
          EXAMPLE_NOT_NULL_CONSTRAINT
        </constraint>
        <constraint constraintType="PRIMARY_KEY">
          CONSTRAINT_NAME: ${constraint_name}, TABLE_NAME: ${table_name}, COLUMN_NAMES:
          ${column_names}
        </constraint>
        <constraint constraintType="FOREIGN_KEY">
          CONSTRAINT_NAME: ${constraint_name}, COLUMN_NAMES: ${column_names}, CHILD_TABLE_NAME:
          ${child_table_name}, CHILD_COLUMN_NAMES: ${child_column_names}, PARENT_TABLE_NAME:
          ${parent_table_name}, PARENT_COLUMN_NAMES: ${parent_column_names}
        </constraint>
        <constraint constraintType="UNIQUE">
          CONSTRAINT_NAME: ${constraint_name}, TABLE_NAME: ${table_name}, COLUMN_NAMES:
          ${column_names}
        </constraint>
      </inlineConstraints>
    </templates>
  </tableCreation>
</syntax>