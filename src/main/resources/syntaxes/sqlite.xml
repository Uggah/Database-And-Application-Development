<?xml version="1.0" encoding="UTF-8" ?>
<syntax>
  <templates>
    <createTable>
      CREATE TABLE ${table_name} (${column_definitions} ${end_of_block_constraints});
    </createTable>
    <columnDefinition>
      ${column_name} ${column_type} ${end_of_line_constraints}
    </columnDefinition>
    <primaryKeyConstraint strategy="END_OF_BLOCK">
      PRIMARY KEY (${column_names})
    </primaryKeyConstraint>
    <foreignKeyConstraint strategy="END_OF_BLOCK">
      FOREIGN KEY ${constraint_name} (${column_names}) REFERENCES ${parent_table_name}
      (${parent_column_names})
    </foreignKeyConstraint>
    <uniqueConstraint strategy="END_OF_BLOCK">
      UNIQUE (${column_names})
    </uniqueConstraint>
    <notNullConstraint strategy="END_OF_LINE">
      NOT NULL
    </notNullConstraint>
  </templates>
</syntax>